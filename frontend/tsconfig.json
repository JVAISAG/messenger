{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/dashboard/dashboard.jsx",
    "src/app/dashboard/rightSide.jsx",
    "src/app/dashboard/left-side.jsx",
    "src/app/dashboard/components/contactCard.jsx",
    "src/app/registration/registration.jsx",
    "src/app/page.jsx",
    "src/app/dashboard/page.jsx",
    "src/app/layout.jsx"
  ],
  "exclude": ["node_modules"]
}
